{
  "name": "Java with GUI",
  "dockerFile": "Dockerfile",
  "forwardPorts": [6901],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack"
      ]
    }
  },
  "postCreateCommand": "/dockerstartup/xvfb.sh && /dockerstartup/vnc_startup.sh"
}








