{
  "name": "Java with GUI",
  "dockerFile": "Dockerfile",
  "forwardPorts": [5901, 6901],
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "{your-terminal-profile-name}"
      }
    }
  },
  "postCreateCommand": "/dockerstartup/vnc_startup.sh"
}









