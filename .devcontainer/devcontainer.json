{
  "name": "Java with GUI",
  "dockerFile": "Dockerfile",
  "forwardPorts": [6901],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "/opt/cs50/extensions/cs50-0.0.1.vsix",
        "/opt/cs50/extensions/explain50-1.0.0.vsix",
        "/opt/cs50/extensions/phpliteadmin-0.0.1.vsix",
        "/opt/cs50/extensions/style50-0.0.1.vsix",
        "cs50.ddb50"      
      ]
    }
  },
  "postCreateCommand": "/dockerstartup/xvfb.sh && /dockerstartup/vnc_startup.sh"
}








